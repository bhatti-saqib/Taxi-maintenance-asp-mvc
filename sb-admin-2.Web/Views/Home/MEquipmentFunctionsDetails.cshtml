                                            new { value = "Fair", text = "Fair"},
                                            new { value = "Poor", text = "Poor"},
                                            new { value = "VeryPoor", text = "Very Poor"},
                                        },
                                        "value",
                                        "text",
                                        2), "-- Please Select --", new { @class = "form-control" })
                                    @Html.ValidationMessageFor(m => m.MT_Gps, "", new { @class = "text-danger" })
                                </div>
                                <div class="form-group">
                                    @Html.Label("4G")
                                    @*@Html.EnumDropDownListFor(model => model.NT_Four_g, "Select 4G", new { @class = "form-control" })*@
                                    @Html.DropDownListFor(m => m.MT_Four_g, new SelectList(
                                    new List<Object>
                                        {
                                            new { value = "VeryGood", text = "Very Good"},
                                            new { value = "Good", text = "Good"},
                                            new { value = "Fair", text = "Fair"},
                                            new { value = "Poor", text = "Poor"},
                                            new { value = "VeryPoor", text = "Very Poor"},
                                        },
                                        "value",
                                        "text",
                                        2), "-- Please Select --", new { @class = "form-control" })
                                    @Html.ValidationMessageFor(m => m.MT_Four_g, "", new { @class = "text-danger" })
