                                <div class="form-group">
                                    @Html.Label("WiFi")
                                    @*@Html.EnumDropDownListFor(model => model.NT_Wifi, "Select WiFi", new { @class = "form-control" })*@
                                    @Html.DropDownListFor(m => m.MT_Wifi, new SelectList(
                                   new List<Object>
                                        {
                                            new { value = "VeryGood", text = "Very Good"},
                                            new { value = "Good", text = "Good"},
                                            new { value = "Fair", text = "Fair"},
                                            new { value = "Poor", text = "Poor"},
                                            new { value = "VeryPoor", text = "Very Poor"},
                                        },
                                        "value",
                                        "text",
                                        2), "-- Please Select --", new { @class = "form-control" })
                                    @Html.ValidationMessageFor(m => m.MT_Wifi, "", new { @class = "text-danger" })
                                </div>
                                <div class="form-group">
                                    @Html.Label("UPS Voltage Test")
                                    @*@Html.EnumDropDownListFor(model => model.NT_VoltageTest, "Select UPS Voltage Test", new { @class = "form-control" })*@
                                    @Html.DropDownListFor(m => m.MT_VoltageTest, new SelectList(
                                    new List<Object>
                                        {
                                            new { value = "VeryGood", text = "Very Good"},
                                            new { value = "Good", text = "Good"},
                                            new { value = "Fair", text = "Fair"},
                                            new { value = "Poor", text = "Poor"},
                                            new { value = "VeryPoor", text = "Very Poor"},
                                        },
                                        "value",
                                        "text",
                                        2), "-- Please Select --", new { @class = "form-control" })
                                    @Html.ValidationMessageFor(m => m.MT_VoltageTest, "", new { @class = "text-danger" })
                                </div>
                                @*<input type="submit" class="btn btn-primary" name="prevBtn" value='Previous' />*@
                                <input type="submit" class="btn btn-primary" name="nextBtn" value='Next' />
                            </form>
                        </div>
                        <!-- /.col-lg-6 (nested) -->
                        @*<div class="col-lg-6">

                            </div>*@
                        <!-- /.col-lg-6 (nested) -->
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
}


