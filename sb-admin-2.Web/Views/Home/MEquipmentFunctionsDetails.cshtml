
@model sb_admin_2.Web.Models.Preventive

@{
    ViewBag.Title = "MEquipmentFunctionsDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("MEquipmentFunctionsDetails", "Home"))
{
    <h2>Preventive Maintenance</h2>

    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">

                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-6">
                            <form role="form">
                                <div class="form-group">
                                    @*<div class="form-inline">*@
                                    @Html.Label("Bronco MDVR")
                                    @*@Html.EnumDropDownListFor(model => model.NT_BroncoMdvrs, "Select Bronco MDVR", new { @class = "form-control" })*@
                                    @Html.DropDownListFor(m => m.MT_BroncoMdvrs, new SelectList(
                                    new List<Object>
                                        {
                                            new { value = "VeryGood", text = "Very Good"},
                                            new { value = "Good", text = "Good"},
                                            new { value = "Fair", text = "Fair"},
                                            new { value = "Poor", text = "Poor"},
                                            new { value = "VeryPoor", text = "Very Poor"},
                                        },
                                        "value",
                                        "text",
                                    2), "-- Please Select --", new { @class = "form-control" })
                                    @*@Html.RadioButton("Replaced HDD", "Yes", false) Recording*@
                                    @Html.ValidationMessageFor(m => m.MT_BroncoMdvrs, "", new { @class = "text-danger" })
                                    @*</div>*@
                                </div>
                                <div class="form-group">
                                    @Html.Label("GPS")
                                    @*@Html.EnumDropDownListFor(model => model.NT_Gps, "Select GPS", new { @class = "form-control" })*@
